{
	"name":"wangaz/contao-isotope-wirecard",
	"description":"Wirecard payment module for Isotope Ecommerce",
	"keywords":["contao", "isotope", "payment", "wirecard"],
	"type":"contao-module",
	"homepage":"http://www.wangaz.com",
	"license":"CC-BY-SA-4.0",
	"authors":[
		{
			"name":"Wangaz.",
			"homepage":"http://www.wangaz.com"
		}
	],
	"support":{
		"issues":"https://github.com/wangaz/contao-isotope-wirecard/issues",
		"source":"https://github.com/wangaz/contao-isotope-wirecard",
		"forum":"https://community.contao.org/de/showthread.php?49734"
	},
	"require":{
		"php":">=5.3",
		"contao/core-bundle":"^3.2 || ^4.3",
		"contao-community-alliance/composer-plugin": "^2.4 || ^3.0",
		"isotope/isotope-core":"^2.0"
	},
	"autoload": {
		"psr-0": {
			"Isotope\\": "src/system/modules/isotope-wirecard/library"
		}
	},
	"replace": {
		"contao-legacy/isotope_wirecard": "*"
	},
	"extra":{
		"contao": {
			"sources": {
				"src/system/modules/isotope-wirecard":"system/modules/isotope-wirecard"
			}
		}
	}
}
